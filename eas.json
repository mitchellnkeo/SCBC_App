{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium",
        "buildConfiguration": "Debug"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m1-medium",
        "distribution": "app-store"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "mitchellnkeo@gmail.com",
        "ascAppId": "6748161420",
        "appleTeamId": "UN9629AFCG"
      }
    }
  }
}
